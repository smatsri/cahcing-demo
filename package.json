{
  "name": "caching-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production yarn build:server && yarn copyfiles && yarn build:next",
    "serve": "cross-env NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"app/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "collections": "^5.1.13",
    "cross-env": "^7.0.3",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "express": "^4.18.2",
    "fastpriorityqueue": "^0.7.4",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/collections": "^5.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/winston": "^2.4.4",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0"
  }
}
